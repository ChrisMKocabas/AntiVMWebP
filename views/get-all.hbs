<div
  style="width:100%;height:100%; ;align-self:center;text-align:left;padding:16px;display: flex;justify-content:center; align-items:flex-start"
>
  <table
    border="1"
    ,style="width:100%;height:100%;align-self:center;flex-direction:column; display: flex;justify-content:center-evenly;"
  >
    <th>ID</th>
    <th>Restaurant ID</th>
    <th>Name</th>
    <th>Cuisine</th>
    <th>Borough</th>
    <th>Address</th>
    <th>Grades</th>

    {{#each data}}
      <tr>
        <td>{{this._id}}</td>
        <td>{{this.restaurant_id}}</td>
        <td>{{this.name}}</td>
        <td>{{this.cuisine}}</td>
        <td>{{this.borough}}</td>
        <td>
          {{this.address.building}}
          {{#each this.address.coord}}{{/each}}
          {{this.address.street}}
          {{this.address.zipcode}}
        </td>
        <td>
          {{#each this.grades}}
            {{#this}}
              <table border="1">
                <tr>
                  <td>Date: {{date}}</td>
                  <td>Grade: {{grade}}</td>
                  <td>Score: {{score}}</td>
                </tr>
              </table>
            {{/this}}
          {{/each}}
        </td>
        {{!-- <td>{{this.publishedDate.$date}}</td> --}}
      </tr>

    {{/each}}
  </table>
</div>